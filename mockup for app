        if len(two_array) == 30:
            two_array.clear()  # clears array so it can be used again
            if l == 1:
                if arearatio < 17.5:
                    l == 0
                    cv2.putText(frame, 'Exit', (0, 50), font, 2, (255, 255, 255), 3, line)
                    zero_array.append(0)
                    print(zero_array)
                    if len(zero_array) == 30:
                        # RETURN TO HOME SCREEN
                        zero_array.clear()  # clears array so it can be used again
                else:
                    cv2.putText(frame, 'Play/Pause', (0, 50), font, 2, (255, 255, 255), 3, line)
                    one_array.append(l)
                    print(one_array)
                    if len(one_array) == 30:
                        this.sendNotification("SPOTIFY_TOGGLE")
                        one_array.clear()  # clears array so it can be used again
            if l == 2:
                cv2.putText(frame, 'Shuffle' , (0, 50), font, 2, (255, 255, 255), 3, line)
                two_array.append(l)
