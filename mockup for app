                five_array.append(l)
                print(five_array)
                if len(five_array) == 30:
                    this.sendNotification("SPOTIFY_PREVIOUS")
                    five_array.clear()  # clears array so it can be used again
            else:
                cv2.putText(frame, 'Spotify Menu', (10, 50), font, 2, (255, 255, 255), 3, cv2.LINE_AA)
