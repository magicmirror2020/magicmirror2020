                if len(two_array) == 30:
                    this.sendNotification("SPOTIFY_SHUFFLE")
                    two_array.clear()  # clears array so it can be used again
            elif l == 3:
                cv2.putText(frame, 'Repeat', (0, 50), font, 2, (255, 255, 255), 3, line)
                three_array.append(l)
                print(three_array)
